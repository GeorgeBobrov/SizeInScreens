{
   "manifest_version": 2,
   "name": "SizeInScreens",
   "description": "Show size of scrollable page in screen sises",
   "version": "1.0",
   "background": {
      "scripts": ["background.js"],
      "persistent": false
   },
   "permissions": [
      "*://*/*", "tabs", "storage", "contextMenus"
   ], 
   "icons": {
      "128": "SizeInScreens.png"
   }
}
