{
   "manifest_version": 2,
   "name": "SizeInScreens",
   "description": "Show size of scrollable page in screen sises",
   "version": "0.5",
   "content_scripts": [ {
      "js": [ "SizeInScreens.js" ],
      "matches": [ "https://habr.com/*" ]
   } ],
   "background": {
      "scripts": ["background.js"],
      "persistent": false
   },
   "permissions": [
      "*://*/*", "tabs", "storage", "contextMenus"
   ], 
   "icons": {
      "128": "SizeInScreens.png"
   }
}
