{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": ["warn", "tab"],
        "no-unused-vars": ["warn", {
            "args": "none",
            "caughtErrors": "none",
            "ignoreRestSiblings": true,
            "vars": "all"
        }],
        "no-multi-spaces": ["warn", { "ignoreEOLComments": true }],
        "no-trailing-spaces": ["warn", { "ignoreComments": true }],
        "eqeqeq": ["warn", "smart"]
    }
}
